<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE gsa-template PUBLIC "-//Art Technology Group, Inc.//DTD General SQL Adapter//EN" "http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
	<header>
        <name>LoyaltyRepository</name>
        <author/>
    </header>
    <item-descriptor name="loyaltyTransaction" display-property="id">
    	<table name="loyalty_transaction" type="primary" id-column-names="id">
    		<property name="id" data-type="string"/>
    		<property name="amount" data-type="int"/>
    		<property name="description" data-type="string"/>
    		<property name="date" column-name="creation_date" data-type="timestamp"/>
    		<property name="profileId" data-type="string"/>
    	</table>
    </item-descriptor>
</gsa-template>